{
  "printWidth": 80,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "all",
  "overrides": [
    {
      "files": ["*.css"],
      "options": {
        "parser": "css",
        "printWidth": 10000,
        "singleQuote": false
      }
    },
    {
      "files": ["*.scss"],
      "options": {
        "parser": "scss",
        "printWidth": 10000,
        "singleQuote": false
      }
    }
  ]
}
