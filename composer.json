    "autoload-dev": {
        "psr-4": {
            "Drupal\\Tests\\openculturas\\": "tests/src"
        }
    },
    "conflict": {
        "drupal/drupal": "*",
        "drupal/ctools": "~4.0.0"
    },
    "replace": {
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "minimum-stability": "alpha",
    "prefer-stable": true,
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "oomphinc/composer-installers-extender": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        },
        "platform": {
            "php": "8.1.16"
        }
    },
    "scripts": {
        "export-content": [
            "drush pm:install default_content --yes",
            "rm -rf web/profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references node --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references media --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references menu_link_content --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush default-content:export-references taxonomy_term --folder=profiles/contrib/openculturas-profile/modules/custom/openculturas_content/content",
            "drush pm:uninstall default_content --yes"
        ],
        "si": [
            "rm -rf web/sites/default/files && mkdir -p web/sites/default/files/translations",
            "drush site:install --yes"
        ],
        "cde" : [
            "drush pm:install config_devel --yes",
            "drush cde openculturas",
            "drush cde openculturas_faq",
            "drush cde openculturas_discussions",
            "drush cde openculturas_map"
        ],
        "php:cs": "phpcs -s",
        "php:cs-fix": "phpcbf",
        "php:phpstan": "phpstan",
        "php:rector": "rector process --dry-run",
        "php:rector-fix": "rector process",
        "php:lint": ["parallel-lint --colors --show-deprecated -e php,module,install,profile,theme profile"],
        "php:qa": ["@php:lint", "@php:cs", "@php:phpstan", "@php:rector"],
        "info_file_normalizer": "@php scripts/info_file_normalizer.php"
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[project-root]/.gitattributes": false,
                "[web-root]/.csslintrc": false,
                "[web-root]/.ht.router.php": false,
                "[web-root]/INSTALL.txt": false,
                "[web-root]/README.md": false,
                "[web-root]/example.gitignore": false,
                "[web-root]/web.config": false,
                "[web-root]/sites/README.txt": false,
                "[web-root]/modules/README.txt": false,
                "[web-root]/profiles/README.txt": false,
                "[web-root]/themes/README.txt": false,
                "[web-root]/sites/development.services.yml": {
                    "mode": "replace",
                    "path": "assets/development.services.yml",
                    "overwrite": false
                },
                "[web-root]/sites/example.sites.php": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "enable-patching": true,
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "patches": {
            "drupal/flag": {
                "Issue #3260640: A11y: Send the status message via drupal.announce": "https://www.drupal.org/files/issues/2022-12-22/flag-3260640-ajax-link-message-announce-8.patch",
                "Issue #3143606: missing global value when saving flagging with form field entry": "https://www.drupal.org/files/issues/2020-05-28/flagging_global-3143606-3.patch",
                "Issue #3059976: 'Unflag not allowed text' not showing up": "https://www.drupal.org/files/issues/2022-12-01/flag-not-allowed-text-3059976-12.patch"
            },
            "drupal/views_infinite_scroll": {
                "Issue #3267139: Make Load more markup more accessible": "https://www.drupal.org/files/issues/2022-07-18/views_infinite_scroll-3267139-missing-pager-headline-10.patch"
            },
            "drupal/core_needs_rebuild": {
                "Issue #2818017: A number of Views combined fields filter operators are currently pointless to an end user": "https://www.drupal.org/files/issues/2023-02-07/2818017-30.patch"
            },
            "drupal/core": {
                "Issue #3221375: Wrong language field labels after drupal_rebuild()": "https://www.drupal.org/files/issues/2021-12-01/3221375-27-workaround.patch",
                "Issue #2845144: User can't reference unpublished content even when they have access to it": "https://www.drupal.org/files/issues/2024-02-13/2845144-87.patch",
                "Issue #2321071: BaseFieldOverride fails to take into account ContentEntityInterface::bundleFieldDefinitions() when invoking onFieldDefinitionUpdate()": "https://www.drupal.org/files/issues/2023-11-02/2321071-71.patch",
                "Issue #3377166: WorkflowDeleteForm should extend EntityDeleteForm": "https://www.drupal.org/files/issues/2023-09-12/3377166-10.patch",
                "Issue #2546212: Entity view/form mode formatter/widget settings have no translation": "https://www.drupal.org/files/issues/2024-04-08/drupal-core-2546212-10.3.x.patch"
            },
            "drupal/entity_reference_revisions": {
                "Issue #2799479: Views doesn't recognize relationship to host": "https://www.drupal.org/files/issues/2023-06-29/entity_reference_revisions-2799479-fix-only.patch"
